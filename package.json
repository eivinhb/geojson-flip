{
  "name": "geojson-flip",
  "version": "1.0.0",
  "description": "Flip lon/lat in geojson documents",
  "main": "flip.js",
  "scripts": {
    "test": "jasmine spec/support/jasmine-runner.js",
    "coverage": "rm -rf coverage && istanbul cover spec/support/jasmine-runner.js",
    "open-coverage": "npm run coverage && open ./coverage/lcov-report/index.html",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "geojson"
  ],
  "author": "eivinhb",
  "license": "ISC",
  "repository": "https://github.com/eivinhb/geojson-flip",
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^2.4.0"
  }
}
